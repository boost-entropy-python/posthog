fun sortableSemver(version) {
  if (version == null) {
    return []
  }
  // Extract version pattern like "1.2.3" from strings like "v1.2.3-alpha"
  // Matches ClickHouse: extract(version, '(\d+(\.\d+)+)')
  let extracted := extractRegex(version, '(\\d+(\\.\\d+)+)')
  if (empty(extracted)) {
    return []
  }
  let parts := splitByString('.', extracted)
  return arrayMap(x -> toInt(x) ?? 0, parts)
}
